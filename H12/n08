package hw12;

interface iAaa {
	public void show();
}
class cbbb{
	public int num=10;
	public void set (int n) {
		num=n;
	}
}
class Cccc extends cbbb implements iAaa{
	public void show() {
		System.out.println("num ="+num);
	}
}
public class class08 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Cccc cc =new Cccc();
		cc.set(s);
		cc.show();

	}

}
