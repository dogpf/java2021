public class FibonacciSequence {
    public static void main(String[] args) {
        int[] fibonacci = new int[10];
        
        // 初始值
        fibonacci[0] = 0;
        fibonacci[1] = 1;
        
        // 計算費氏數列
        for (int i = 2; i < 10; i++) {
            fibonacci[i] = fibonacci[i - 1] + fibonacci[i - 2];
        }
        
        // 顯示前10個數值
        for (int i = 0; i < 10; i++) {
            System.out.println(fibonacci[i]);
        }
    }
}
